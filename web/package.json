{
  "name": "nessy",
  "collaborators": [
    "nathsou <nathsou@users.noreply.github.com>"
  ],
  "version": "0.1.0",
  "files": [
    "nessy_bg.wasm",
    "nessy.js",
    "nessy.d.ts"
  ],
  "scripts": {
    "wasm": "wasm-pack build ../crate --target web",
    "copy": "mkdir -p ./public && rm -rf ./public/wasm && cp -rf ../crate/pkg ./public/wasm",
    "dev": "npm run wasm && npm run copy && vite",
    "build": "vite build --base=/nessy/"
  },
  "module": "nessy.js",
  "types": "nessy.d.ts",
  "sideEffects": false,
  "devDependencies": {
    "vite": "^4.3.9",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "browser-fs-access": "^0.34.1"
  }
}